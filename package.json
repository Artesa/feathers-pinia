    "docs:copy-assets": "cp ./docs/feathers-pinia.png ./docs/.vitepress/dist/",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "prepare": "tsc",
    "serve": "vite preview",
    "compile": "shx rm -rf dist/ && tsc",
