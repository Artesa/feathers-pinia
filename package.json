    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.6.0",
    "feathers-memory": "^4.1.0",
    "jest": "^27.5.1",
    "path": "^0.12.7",
    "pinia": "^2.0.13",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.6.2",
    "shx": "^0.3.4",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vitepress": "^0.22.3",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.7"
  },
